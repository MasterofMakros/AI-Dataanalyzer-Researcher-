# Conductor N8N Stack - Environment Variables
# Copy this file to .env and customize

# ============ SECURITY ============
# Generate with: openssl rand -hex 32
N8N_ENCRYPTION_KEY=change_me_to_random_32_char_hex_string
POSTGRES_PASSWORD=n8n_secure_password_2025
REDIS_PASSWORD=redis_secure_password_2025

# ============ NEXTCLOUD ============
NC_ADMIN_PASSWORD=admin_change_this_immediately
NC_DB_PASSWORD=nextcloud_db_password_2025
NC_DB_ROOT_PASSWORD=root_password_change_me

# ============ N8N ============
# Your Pi's IP or domain
N8N_HOST=192.168.1.254
WEBHOOK_URL=http://192.168.1.254:5678/

# ============ TIMEZONE ============
GENERIC_TIMEZONE=Europe/Berlin
